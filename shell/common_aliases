# aliases

alias ll='ls -Fhl'
alias ls='ls -Fh'

alias ll='ls -Fhl'
alias ls='ls -Fh'
alias py='python'
alias py3='python3'
alias pip3='pip-3.6'
alias pyc='python -m compileall'
alias pyc3='python3 -m compileall'
alias jk='jekyll'
alias jk2='jekyll _2.5.3_'
alias npmlist='npm list --depth=0'
alias npmlistg='npm list -g --depth=0'
alias wgetnc="wget --no-check-certificate"
